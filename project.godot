; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Sprite",
"class": "Building",
"language": "GDScript",
"path": "res://entities/Building.gd"
}, {
"base": "Resource",
"class": "COData",
"language": "GDScript",
"path": "res://resources/COData.gd"
}, {
"base": "TileMap",
"class": "PathTileMap",
"language": "GDScript",
"path": "res://engine/PathTileMap.gd"
}, {
"base": "Resource",
"class": "PowerUp",
"language": "GDScript",
"path": "res://resources/PowerUp.gd"
}, {
"base": "Resource",
"class": "Team",
"language": "GDScript",
"path": "res://entities/Team.gd"
}, {
"base": "Path2D",
"class": "Unit",
"language": "GDScript",
"path": "res://entities/Unit.gd"
} ]
_global_script_class_icons={
"Building": "",
"COData": "",
"PathTileMap": "",
"PowerUp": "",
"Team": "",
"Unit": ""
}

[application]

config/name="Mech Wars"
run/main_scene="res://engine/Main.tscn"
config/icon="res://icon.png"

[autoload]

gl="*res://globals/Globals.gd"
sp="*res://globals/Sprites.gd"
signals="*res://globals/Signals.gd"
astar="*res://globals/AStar.gd"

[debug]

gdscript/warnings/integer_division=false

[display]

window/size/width=960
window/size/height=540
window/size/resizable=false
window/size/fullscreen=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[input]

click={
